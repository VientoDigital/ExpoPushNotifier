{
    "name": "vientodigital/expopushnotifier",
    "description": ":Expo Push Notification Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Yoalli Dominguez",
            "email": "victoryoalli@gmail.com",
            "homepage": "http://viento.digital"
        }
    ],
    "homepage": "https://github.com/vientodigital/expopushnotifier",
    "keywords": [
        "Laravel",
        "ExpoPushNotifier"
    ],
    "require": {
        "illuminate/support": "^5.5|^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "VientoDigital\\ExpoPushNotifier\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "VientoDigital\\ExpoPushNotifier\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VientoDigital\\ExpoPushNotifier\\ExpoPushNotifierServiceProvider"
            ],
            "aliases": {
                "ExpoPushNotifier": "VientoDigital\\ExpoPushNotifier\\Facades\\ExpoPushNotifier"
            }
        }
    }
}